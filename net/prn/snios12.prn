                ; SNIOS Prefix (jump table) for CP/NET 1.2 on CP/M 2.2
                
                	maclib z80
                
                	extrn	NTWKIN, NTWKST, CNFTBL, SNDMSG, RCVMSG, NTWKER, NTWKBT
                
                	cseg
                ;	Jump vector for SNIOS entry points
 0000 C30000    	jmp	NTWKIN	; network initialization
 0003 C30000    	jmp	NTWKST	; network status
 0006 C30000    	jmp	CNFTBL	; return config table addr
 0009 C30000    	jmp	SNDMSG 	; send message on network
 000C C30000    	jmp	RCVMSG	; receive message from network
 000F C30000    	jmp	NTWKER	; network error
 0012 C30000    	jmp	NTWKBT	; network warm boot
                
 0015           	end
